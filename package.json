{
  "private": true,
  "name": "web-steps",
  "version": "0.2.8",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "node scripts/bootstrap.js",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "debug": "node scripts/debug.js",
    "create-api-doc": "api-documenter markdown --input-folder ./temp --output-folder ./temp/markdown",
    "size-runtime": "node scripts/build.js runtime-dom size-check -p -f global",
    "size-compiler": "node scripts/build.js compiler-dom -p -f global",
    "size": "yarn size-runtime && yarn size-compiler",
    "lint": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "debug:test": "cross-env NODE_ENV=test DEBUG_PORT=32000 jest --runInBand",
    "release": "node scripts/release.js",
    "unpublish": "node scripts/unpublish.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.11.0",
    "@microsoft/api-extractor": "^7.12.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-replace": "^2.3.0",
    "@types/ali-oss": "^6.0.4",
    "@types/co": "^4.6.2",
    "@types/express": "^4.17.2",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^24.0.21",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash": "^4.14.149",
    "@types/memory-fs": "^0.3.2",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/minimist": "^1.2.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/puppeteer-core": "^2.0.0",
    "@types/require-from-string": "^1.2.0",
    "@types/rimraf": "^2.0.3",
    "@types/semver": "^7.1.0",
    "@types/serialize-javascript": "^1.5.0",
    "@types/webpack": "^4.41.3",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "axios": "^0.19.2",
    "brotli": "^1.3.2",
    "cache-loader": "^4.1.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog-cli": "^2.0.31",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "execa": "^4.0.0",
    "jest": "^26.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.2",
    "prettier": "~1.14.0",
    "puppeteer-core": "^2.1.0",
    "rollup": "^1.29.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "thread-loader": "^2.1.3",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "~4.0.5",
    "vue": "^2.6.11",
    "vue-eslint-parser": "^7.0.0",
    "vue-loader": "^15.8.3",
    "vue-router": "^3.1.5",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webpack": "^4.41.5"
  }
}
